<div class="panel panel-primary">
    <div class="panel-heading">
        <h3 class="panel-title">
            {{post.getPostTitle()|raw}}
        </h3>
    </div>

    <div class="panel-body">
        {{post.getPostImage()}}
        {#        {% include 'MRussoCmsBundle::Front/templates/'~post.getPostTemplate().getPostTemplateFile()~'.html.twig' %}#}
    </div>
    <div class='panel-footer'>
        <div class='row'>
            <div class='col-xs-12'>
                {{post.getPostDate().format('d/m/Y')}}
            </div>

        </div>
    </div>
</div>
